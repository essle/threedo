{
  "extends": [
    "neki/ts-react"
  ],
  "rules": {
    "import/order": [
      "warn",
      {
        "newlines-between": "always",
        "groups": [
          "builtin",
          "external",
          "parent",
          "sibling",
          "index"
        ],
        "alphabetize": {
          "order": "asc",
          "caseInsensitive": true
        },
        "pathGroups": [
          {
            "pattern": "~**/**",
            "group": "parent"
          },
          {
            "pattern": "./styled",
            "group": "index"
          }
        ]
      }
    ]
  }
}