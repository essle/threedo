  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "app/",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "paths": {
      "~t/*": ["src/server/types/*"]
